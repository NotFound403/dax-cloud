<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.felord.security.mapper.UserRoleMapper">
    <select id="check" resultType="cn.felord.common.entity.securityserver.UserRole">
        SELECT *
        FROM user_role
        WHERE userId = #{userId}  AND enabled='1'
    </select>
</mapper>